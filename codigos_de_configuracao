$ git init
// inicializar o git
$ git status
//verificar o status do git
$ git add meu_documento.txt
//adicionar o arquivo criado ao git
$ git config --local user.name "Marcos Meireles"
c//configurando o usuário
$ git config user.name
//verificando o usuario
$ git config --local user.email "meuemail@gmail.com"
//configurando o e-mail do usuário
$ git status
//verificando o status
$ git config user.email
//verificando o e-mail
$ git add meu_documento
//adiciona somente o arquivo específico 
$ git add .
//adicionar todos os arquivos
$ git commit -m "criando meu documento"
//consolidando as alterações
$ git log
// verificando o log do commit
$ git log --oneline
// verificando o log do commit em uma linha
$ git add .
$ git commit -m "criando o gitignore para documentos importantes"
$ git log
$ git remote add  'C:/Users/Aluno Noite/Documents/git_java_noturno/servidor/'
//criando servidor
$ git remote 'C:/Users/Aluno Noite/Documents/git_java_noturno/servidor/'
//verificando o servidor
$ git remote add servidor_local 'C:/Users/Aluno Noite/Documents/git_java_noturno/servidor/'
$ git remote
$ git push servidor_local master
// empurrando os arquivos para o servidor


$ git init

// comando de inicializar o git


$ git status

//comando para verificar o status do git


$ git add meu_documento.txt

//comando para adicionar o arquivo criado ao git


$ git config --local user.name "José Luiz"

//comando para configurar o usuário


$ git config user.name

//comando para verificar o usuario


$ git config --local user.email "jl@gmail.com"

//comando para configurar o e-mail do usuário


$ git status

//comando para verificar o status


$ git config user.email

//comando para verificando o e-mail


$ git add nome_do_ meu_documento

//comando para adicionar somente o arquivo específico 


$ git add .

//comando para adicionar todos os arquivos


$ git commit -m "criando meu documento"

//comando para salvar as alterações


$ git log

// comando para verificar o log do commit


$ git log --oneline

// comando para verificar o log do commit em uma linha


$ git commit -m "criando o gitignore para documentos importantes"

// comando para comentar os arquivos


$ git log

//comando para verificar se arquivos estão consolidados


$ git init –bare

//comando para criar e mostrar o servidor, na pasta do SERVIDOR


$ git remote add servidor_local 'C:/Users/Aluno Noite/Documents/git_java_noturno/servidor/'

//comando para adicionar servidor com nome servidor_local


$git remote

// confirmar a criação do servidor


 $ git push servidor_local master

// empurrando os arquivos para o servidor


$git clone  'C:/Users/Aluno Noite/Documents/git_java_noturno/servidor/'

// comando para clonar um servidor


cd servidor

// mudando de de past.a 


$git pull origin master

//comando para pegar as alterações feitas no arquivo local


$ git remote 'C:/Users/Aluno Noite/Documents/git_java_noturno/servidor/'

//comando para verificar se o servidor foi criado


$ git remote add servidor_local 'C:/Users/Aluno Noite/Documents/git_java_noturno/servidor/'

//comando para enviar os arquivos para o servidor.


